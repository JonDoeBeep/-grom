# @grom Discord Bot - Environment Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token (required)
DISCORD_TOKEN=your_discord_token_here

# OpenAI-compatible API settings (required)
# Works with NanoGPT, OpenAI, Ollama, LM Studio, etc.
OPENAI_API_KEY=your_api_key_here
OPENAI_BASE_URL=https://api.nano-gpt.com/v1
OPENAI_MODEL=gpt-4o-mini

# Bot settings
COMMAND_PREFIX=/
DEBUG_MODE=false
